<%@ page language="java" import="java.util.*" pageEncoding="ISO-8859-1"%>
<form name='mform' id='mform' action="servlet/ToolMenuDispatchServlet" method="post">
<input type='hidden' name='target' value='description.jsp'/><br>
<input type='hidden' name='basePath' value='/WEB-INF/jsp/estatetools/life'/>
<input type='hidden' name='pageView' value='cancel'/>
<table>
<tr><td>
	<input type="image" src="btns/BtnDescription.png" alt="Description"
		onclick="document.mform.target.value='description.jsp'"
		onmouseover="this.src='btns/BtnDescriptionMouseOver.png';"
		onmouseout="this.src='btns/BtnDescription.png';"
		onmousedown="this.src='btns/BtnDescriptionMouseDown.png';"/>
</td></tr>
<tr><td align='center'>
	<input type="image" src="btns/BtnSetup.png" alt="tool"
		onclick="document.mform.target.value='LifeToolServlet';document.mform.basePath.value='/servlet'"
		onmouseover="this.src='btns/BtnSetupMouseOver.png';"
		onmouseout="this.src='btns/BtnSetup.png';"
		onmousedown="this.src='btns/BtnSetupMouseDown.png';"/>
</td></tr>
<tr><td align='center'>
	<input type="image" src="btns/BtnAssets.png" alt="Assets"
		onclick="document.mform.target.value='LifeAssetServlet';document.mform.basePath.value='/servlet'"
		onmouseover="this.src='btns/BtnAssetsMouseOver.png';"
		onmouseout="this.src='btns/BtnAssets.png';"
		onmousedown="this.src='btns/BtnAssetsMouseDown.png';"/>
</td></tr>
<tr><td align='center'>
	<input type="image" src="btns/BtnSummary.png" alt="Summary"
		onclick="document.mform.target.value='LifeSummaryServlet';document.mform.basePath.value='/servlet'"
		onmouseover="this.src='btns/BtnSummaryMouseOver.png';"
		onmouseout="this.src='btns/BtnSummary.png';"
		onmousedown="this.src='btns/BtnSummaryMouseDown.png';"/>
</td></tr>
<tr><td align='center'>
	<input type="image" src="btns/BtnReport.png" alt="Report"
		onclick="document.mform.target.value='report.jsp'"
		onmouseover="this.src='btns/BtnReportMouseOver.png';"
		onmouseout="this.src='btns/BtnReport.png';"
		onmousedown="this.src='btns/BtnReportMouseDown.png';"/>
</td></tr>
<tr><td align='center'>
	<input type="image" src="btns/BtnSave.png" alt="Save"
		onclick="document.mform.target.value='LifeProcessServlet';
		document.mform.basePath.value='/servlet';document.mform.pageView.value='save'"
		onmouseover="this.src='btns/BtnSaveMouseOver.png';"
		onmouseout="this.src='btns/BtnSave.png';"
		onmousedown="this.src='btns/BtnSaveMouseDown.png';"/>
</td></tr>
<tr><td align='center'>
	<input type="image" src="btns/BtnCancel.png" alt="Cancel"
		onclick="document.mform.target.value='LifeProcessServlet';
		document.mform.basePath.value='/servlet';document.mform.pageView.value='cancel'"
		onmouseover="this.src='btns/BtnCancelMouseOver.png';"
		onmouseout="this.src='btns/BtnCancel.png';"
		onmousedown="this.src='btns/BtnCancelMouseDown.png';"/>
</td></tr>
</table>
</form>
